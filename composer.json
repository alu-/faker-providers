{
    "name": "alu/faker-providers",
    "description": "A collection of custom providers for the fzaninotto/Faker library",
    "keywords": [
        "faker",
        "providers",
        "fixtures",
        "data"
    ],
    "type": "library",
    "autoload": {
        "psr-4": { "Alu\\Faker\\": "src/Faker" }
    },
    "autoload-dev": {
        "psr-4": { "Alu\\Tests\\Faker\\": "tests/unit/Faker" }
    },
    "require": {
        "php": "^7.0",
        "fzaninotto/faker": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Andersson",
            "email": "alu@byteberry.net"
        }
    ]
}
